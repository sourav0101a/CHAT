    onSaveClick() {
        console.log('148778 :');
        this.isLoaded = !this.isLoaded;
    }

    handleSuccess() {
        this.isLoaded = !this.isLoaded;

    this.showEditField = false;
        const event = new ShowToastEvent({
      title: "Success",
      message: "Record has been updated successfully.",
      variant: "success",
      mode: "dismissable"
        });
        this.dispatchEvent(event);
        this.closeQuickAction();
    }
    checkfieldName(event) {
        let currentValue = event.target.value;
    this.valHandoverContract = currentValue;
    if (currentValue == "Yes") {
            this.disableInputField = false;
    } else {
            this.disableInputField = true;
        }
    }
    onCancelClick() {
        // this.showEditField = !this.showEditField;
    }
    checkfieldGroupLicense(event) {
        let currentValue = event.target.value;
    this.valGroupLicense = currentValue;
    if (currentValue == "No" || currentValue == "Not Stated" || currentValue == "" ) {
            this.disableGroupRelatedFieldOne = true;
            this.disableGroupRelatedFieldTwo = false;
    } else if (currentValue == "Yes") {
            this.disableGroupRelatedFieldOne = false;
            this.disableGroupRelatedFieldTwo = true;
        }
    }
    checkfieldGroupCoverage(event) {
        let currentValue = event.target.value;
    this.valScopeofGroupCoverage = currentValue;
    if (
      currentValue == "Heritage LSEG (excluding certain entities)" ||
      currentValue == "Heritage RFT (excluding certain entities)" ||
      currentValue == "New LSEG (excluding certain entities)"
    ) {
            this.disableGroupRelatedFieldThree = false;
            this.disableGroupRelatedFieldFour = true;
            this.disableGroupRelatedFieldFive = true;
    } else if (currentValue == "Limited permitted entities only") {
            this.disableGroupRelatedFieldFour = false;
            this.disableGroupRelatedFieldThree = true;
            this.disableGroupRelatedFieldFive = true;
    } else if (currentValue == "Other") {
            this.disableGroupRelatedFieldFive = false;
            this.disableGroupRelatedFieldThree = true;
            this.disableGroupRelatedFieldFour = true;
    } else {
            this.disableGroupRelatedFieldThree = true;
            this.disableGroupRelatedFieldFour = true;
            this.disableGroupRelatedFieldFive = true;
        }
    }
    checkfieldThirdParty(event) {
        let currentValue = event.target.value;
    this.valSupplieracceptstheuseofthirdpartie = currentValue;
    if (currentValue == "Yes") {
            this.disableGroupRelatedFieldSix = false;
    } else {
            this.disableGroupRelatedFieldSix = true;
        }
    }
    checkfieldSubscriber(event) {
        let currentValue = event.target.value;
    this.valRestrictionsforsubscribercharges = currentValue;
    if (currentValue == "Yes") {
            this.disableGroupRelatedFieldSeven = false;
    } else {
            this.disableGroupRelatedFieldSeven = true;
        }
    }
    checkfieldContract(event) {
        let currentValue = event.target.value;
    this.valContractualPriceIncrease = currentValue;
    if (currentValue == "Yes") {
            this.disableGroupRelatedFieldEight = false;
    } else {
            this.disableGroupRelatedFieldEight = true;
        }
    }
    checkfieldWebsite(event) {
