    handleRegulatoryRequirements(event) {
      let currentValue = event.target.value;
      this.valRegulatoryRequirements = currentValue;
      if (currentValue == "No" || currentValue == '') {
        this.disableRegulatoryRequirements = true;
      } else {
        this.disableRegulatoryRequirements = false;
      }
    }
