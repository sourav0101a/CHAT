    connectedCallback(){  
      console.log('148778 :');
      console.log('testSunitha');
      if(this.ValDateVendor == null || this.ValDateVendor == "undefined"){
        this.disableLastComVendor = true;
        this.disableLastComVendorOther = true;
        this.ValLastComVendor = "";
        this.ValLastComVendorOther = "";
      } else{
        this.disableLastComVendor = false;
        if(this.ValLastComVendor =="Other"){
          this.disableLastComVendorOther = false;
        } else {
          this.disableLastComVendorOther = true;
          this.ValLastComVendorOther = "";
        }
      }
      console.log("disableLastComVendor+++"+this.ValDateVendor);
  
    }
    handleDateVendor(event) {
      let currentValue = event.target.value;
      this.ValDateVendor = currentValue;
      if (currentValue == null || this.ValDateVendor == "undefined") {
        this.ValLastComVendor = '';
        this.ValLastComVendorOther = '';
        this.disableLastComVendor = true;
        this.disableLastComVendorOther = true;
        
      } else {
        this.disableLastComVendor = false;
        if(this.ValLastComVendor =="Other"){
          this.disableLastComVendorOther = false;
        } else {
          this.ValLastComVendorOther = '';
          this.disableLastComVendorOther = true;
          
        }
      }
    }
    handleLastComVendor(event) {
      let currentValue = event.target.value;
      this.ValLastComVendor = currentValue;
      if (currentValue == "Other") {
        this.disableLastComVendorOther = false;     
      } else {      
        this.disableLastComVendorOther = true;  
        this.ValLastComVendorOther = "";   
      }
    }
    handleLastComVendorOther(event) {
      let currentValue = event.target.value;
      this.ValLastComVendorOther = currentValue;
    }

