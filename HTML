    wiredRecord({ error, data }) {
        if (data) {
            
      if ( data.fields.Group_License__c.value == "No" || data.fields.Group_License__c.value == "Not Stated" 
      ) {
                this.disableGroupRelatedFieldOne = true;
                this.disableGroupRelatedFieldTwo = false;
      } else if (data.fields.Group_License__c.value == "Yes") {
                this.disableGroupRelatedFieldOne = false;
                this.disableGroupRelatedFieldTwo = true;
            }

      if(data.fields.Last_Commitment_Date_Vendor__c.value == null || data.fields.Last_Commitment_Date_Vendor__c.value == "undefined"){
        this.disableLastComVendor = true;
        this.disableLastComVendorOther = true;      
      } else {
        this.disableLastComVendor = false;
        if(data.fields.Reason_Last_Commitment_Vendor__c.value == "Other"){
          this.disableLastComVendorOther = false;
        }else{
          this.disableLastComVendorOther = true;
        }
      }
      if (
        data.fields.Scope_of_Group_Coverage__c.value ==
          "Heritage LSEG (excluding certain entities)" ||
        data.fields.Scope_of_Group_Coverage__c.value ==
          "Heritage RFT (excluding certain entities)" ||
        data.fields.Scope_of_Group_Coverage__c.value ==
          "New LSEG (excluding certain entities)"
      ) {
                this.disableGroupRelatedFieldThree = false;
                this.disableGroupRelatedFieldFour = true;
                this.disableGroupRelatedFieldFive = true;
      } else if (
        data.fields.Scope_of_Group_Coverage__c.value ==
        "Limited permitted entities only"
      ) {
                this.disableGroupRelatedFieldFour = false;
                this.disableGroupRelatedFieldThree = true;
                this.disableGroupRelatedFieldFive = true;
      } else if (data.fields.Scope_of_Group_Coverage__c.value == "Other") {
                this.disableGroupRelatedFieldFive = false;
                this.disableGroupRelatedFieldThree = true;
                this.disableGroupRelatedFieldFour = true;
      } else if ( data.fields.Subscriber_Agreement_Required__c.value == 'No' || data.fields.Subscriber_Agreement_Required__c.value == 'Not Stated' || data.fields.Subscriber_Agreement_Required__c.value == null) {
        this.disableGroupRelatedFieldTen = true;
      } else {
                this.disableGroupRelatedFieldThree = true;
                this.disableGroupRelatedFieldFour = true;
                this.disableGroupRelatedFieldFive = true;
